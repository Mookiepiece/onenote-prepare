{
  "name": "pre-onenote",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "electron-webpack dev",
    "compile": "electron-webpack",
    "PRIVATED_dist": "yarn compile && electron-builder",
    "PRIVATED_dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
