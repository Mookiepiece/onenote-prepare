      "slate-history"
    ],
    "renderer": {
      "webpackConfig": "webpack.renderer.js"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@reach/router": "^1.3.3",
    "antd": "^4.0.1",
    "is-hotkey": "^0.1.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "slate": "^0.57.1",
    "slate-history": "^0.57.1",
    "slate-react": "^0.57.1",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "electron": "^8.0.2",
    "electron-builder": "^22.3.2",
    "electron-webpack": "^2.7.4",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "webpack": "~4.35.3"
